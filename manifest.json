{
  "name": "store-maintenance",
  "vendor": "vtex",
  "version": "0.0.1",
  "title": "Maintenance Mode application",
  "description": "Installing this app will show a maintenance page on the site",
  "mustUpdateAt": "2021-11-23",
  "categories": [],
  "dependencies": {
    "vtex.rewriter": "1.x",
    "vtex.store": "2.x",
    "vtex.flex-layout": "0.x",
    "vtex.rich-text": "0.x",
    "vtex.store-header": "2.x",
    "vtex.store-footer": "2.x"
  },
  "builders": {
    "node": "6.x",
    "docs": "0.x",
    "store": "0.x",
    "pixel": "0.x",
    "checkout-ui-custom": "0.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "app.io.vtex.com",
        "path": "*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "vtex.rewriter:route-evaluation"
    },
    {
      "name": "vtex.rewriter:resolve-graphql"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
